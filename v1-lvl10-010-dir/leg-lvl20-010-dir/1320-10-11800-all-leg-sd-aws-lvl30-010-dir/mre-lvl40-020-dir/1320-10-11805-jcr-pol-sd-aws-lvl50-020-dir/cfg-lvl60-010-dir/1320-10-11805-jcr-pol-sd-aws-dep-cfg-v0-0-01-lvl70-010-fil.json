{
  "resourceMetadataKEY": "yes",
  "resourceRoleNameKEY": "sambar-eks-service-1320-10-rol-sd",
  "resourceDescriptionKEY": "This Policy is attached to the sambar-eks-service-1320-10-rol-sd IAM Role which supports EKS Cluster to run.",
  "resourceStatementStructureKEY": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "ec2:CreateNetworkInterfacePermission",
        "Resource": "*",
        "Condition": {
          "ForAnyValue:StringEquals": {
            "ec2:ResourceTag/eks:eni:owner": "eks-vpc-resource-controller"
          }
        }
      },
      {
        "Effect": "Allow",
        "Action": [
          "ec2:CreateNetworkInterface",
          "ec2:DetachNetworkInterface",
          "ec2:ModifyNetworkInterfaceAttribute",
          "ec2:DeleteNetworkInterface",
          "ec2:AttachNetworkInterface",
          "ec2:UnassignPrivateIpAddresses",
          "ec2:AssignPrivateIpAddresses"
        ],
        "Resource": "*"
      }
    ]
  },
  "resourceTagKEY": {
    "aliasNameKEY": "none",
    "botNameKEY": "botname",
    "ifqridKEY": "1320-10-11805-jcr-pol-sd-aws",
    "resourceTypeKEY": "pol",
    "blockNumberKEY": "1320-10",
    "applicationNameKEY": "sambar",
    "environmentIDKEY": "sd",
    "regionCodeKEY": "eu-west-2",
    "deploymentTypeKEY": "sdk"
  }
}
